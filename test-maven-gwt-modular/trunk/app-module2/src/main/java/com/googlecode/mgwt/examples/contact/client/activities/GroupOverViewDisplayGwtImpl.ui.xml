<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">
	
	<mgwt:LayoutPanel>
		<mgwt:HeaderPanel>
		
			<mgwt:center><g:HTML>Groups</g:HTML></mgwt:center>
			
		</mgwt:HeaderPanel>
		<mgwt:ScrollPanel scrollingEnabledX="false">
			
				<mgwt:CellList ui:field="list"/>
			
		</mgwt:ScrollPanel>
		
		<mgwt:ButtonBar>
			<mgwt:BarButton ui:field="plusButton" type="plus"></mgwt:BarButton>
			<mgwt:BarButton ui:field="editButton" type="organize"></mgwt:BarButton>
		</mgwt:ButtonBar>
	</mgwt:LayoutPanel>
</ui:UiBinder> 