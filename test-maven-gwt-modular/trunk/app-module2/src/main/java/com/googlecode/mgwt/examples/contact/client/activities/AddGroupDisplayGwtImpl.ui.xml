<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">

	<mgwt:LayoutPanel>
		<mgwt:HeaderPanel>
			<mgwt:left> 
				<mgwt:HeaderButton text="back" ui:field="leftButton" backButton="true"></mgwt:HeaderButton>
			</mgwt:left>
			<mgwt:center>
				<g:HTML>Add Group</g:HTML>
			</mgwt:center>
		</mgwt:HeaderPanel>

		<mgwt:ScrollPanel scrollingEnabledX="false">

			<g:FlowPanel>
				<mgwt:WidgetList round="true">
					<mgwt:MTextBox ui:field="groupName"></mgwt:MTextBox>
					
					
				</mgwt:WidgetList>
				
			<mgwt:Button text="Add" ui:field="addButton"></mgwt:Button>




			</g:FlowPanel>

		</mgwt:ScrollPanel>
		



	</mgwt:LayoutPanel>

</ui:UiBinder> 