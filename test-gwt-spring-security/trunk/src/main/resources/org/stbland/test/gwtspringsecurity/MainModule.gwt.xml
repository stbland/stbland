<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="gwtspringsecurity">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.user.theme.dark.Dark" />

	<!-- Model View Presenter -->
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />

	<!-- Use ClientFactoryImpl by default -->
	<replace-with
		class="org.stbland.test.gwtspringsecurity.client.factory.ClientFactoryImpl">
		<when-type-is
			class="org.stbland.test.gwtspringsecurity.client.factory.ClientFactory" />
	</replace-with>

	<entry-point
		class="org.stbland.test.gwtspringsecurity.client.MainModuleEntryPoint" />

	<super-source path="translatable"/>

	<source path="client" />
	<source path="shared" />

</module>